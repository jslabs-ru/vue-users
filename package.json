{
  "name": "vue-users",
  "version": "0.0.1",
  "description": "Vue.js quick start",
  "scripts": {
    "serve": "npm run server-api | npm run vcli-service",
    "vcli-service": "vue-cli-service --port 8081 serve",
    "build": "vue-cli-service build",
    "build-with-webpack": "gulp buildWithWebpack",
    "watch": "HMR=1 PORT=8081 nodemon ./server",
    "start": "node ./server-prod",
    "server-api": "PORT=8888 nodemon ./server-api",
    "test": "RUN_MOCHA_TESTS=1 node ./runner",
    "test-with-puppeteer": "./node_modules/mocha/bin/mocha --timeout 5000 --require @babel/register ./test/puppeteer/index.js"
  },
  "keywords": [
    "vue.js"
  ],
  "author": "Denis Savenok <denissavenok@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@vue/cli-service": "^4.5.10",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "chai-match": "^1.1.1",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.5",
    "console-stamp": "^0.2.9",
    "css-loader": "^5.0.1",
    "gulp": "^4.0.2",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "puppeteer": "^7.0.1",
    "rimraf": "^3.0.2",
    "sleep": "^6.3.0",
    "style-loader": "^2.0.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.13.6",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.21.2",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuedraggable": "^2.24.3"
  },
  "main": "server.js",
  "engines": {
    "node": "12.x"
  }
}
